{
    "name": "codecraft-be",
    "version": "1.0.0",
    "description": "",
    "main": "dist/src/main",
    "scripts": {
        "build": "nest build",
        "start": "nest start",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "dev:auth": "nest start auth-service --watch",
        "dev:exam": "nest start exam-service --watch",
        "dev:finance": "nest start finance-service --watch",
        "dev:r2": "nest start r2-service --watch",
        "dev:gateway": "nest start gateway-service --watch",
        "dev:ocr": "nest start ocr-service --watch",
        "dev:ocr-python": "cd apps/ocr-service && PYTHONPATH=src:../../olmocr OLMOCR_PATH=../../olmocr python -m src.backend.main",
        "dev:ai": "cd apps/ai-service && PYTHONPATH=src SSL_CERT_FILE=$(./venv/bin/python -c 'import certifi; print(certifi.where())') ./venv/bin/uvicorn src.backend.main:app --reload --port 8000",
        "dev:all": "concurrently \"pnpm dev:gateway\" \"pnpm dev:auth\" \"pnpm dev:exam\" \"pnpm dev:finance\" \"pnpm dev:r2\" \"pnpm dev:ai\"",
        "prisma:merge": "prisma-merge -b libs/database/src/base.prisma -s 'libs/database/src/models/*.prisma' -o libs/database/src/schema.prisma",
        "prisma:generate": "prisma generate --schema=libs/database/src/schema.prisma",
        "prisma:migrate": "pnpm prisma:merge && prisma migrate dev --schema=libs/database/src/schema.prisma",
        "prisma:push": "pnpm prisma:merge && prisma db push --schema=libs/database/src/schema.prisma",
        "prisma:studio": "prisma studio --schema=libs/database/src/schema.prisma",
        "prisma:deploy": "pnpm prisma:merge && prisma migrate deploy --schema=libs/database/src/schema.prisma",
        "prisma:seed": "npx ts-node libs/database/prisma/seed.ts",
        "start:prod": "node dist/apps/codecraft-be/main"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "packageManager": "pnpm@10.11.0",
    "dependencies": {
        "@aws-sdk/client-s3": "^3.896.0",
        "@google/genai": "^1.19.0",
        "@grpc/grpc-js": "^1.14.3",
        "@grpc/proto-loader": "^0.8.0",
        "@nestjs/axios": "^4.0.1",
        "@nestjs/common": "^11.1.3",
        "@nestjs/config": "^4.0.2",
        "@nestjs/core": "^11.1.3",
        "@nestjs/jwt": "^11.0.0",
        "@nestjs/microservices": "^11.1.10",
        "@nestjs/passport": "^11.0.5",
        "@nestjs/platform-express": "^11.1.3",
        "@nestjs/swagger": "^11.2.0",
        "@nestjs/throttler": "^6.5.0",
        "@prisma/client": "^6.16.2",
        "@techstark/opencv-js": "4.12.0-release.1",
        "amqp-connection-manager": "^5.0.0",
        "amqplib": "^0.10.9",
        "axios": "^1.12.2",
        "bcryptjs": "^3.0.2",
